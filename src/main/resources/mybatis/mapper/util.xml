<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="mybatis.mapper.util">

	<select id="selectAnnounces" resultType="announceVO">
		<![CDATA[
		select id, `order`, content, createdAt from announce
		 order by `order` asc
		 limit 2
		]]>
		
	</select>

</mapper>
